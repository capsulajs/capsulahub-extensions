  },
  "homepage": "https://github.com/capsulajs/web-components#readme",
  "devDependencies": {
    "husky": "^1.3.1",
    "lerna": "^3.13.2",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0"
  },
  "dependencies": {
    "@capsulajs/capsulahub-ui": "^0.5.0"
  }
