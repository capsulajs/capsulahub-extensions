{
  "name": "@capsulajs/web-components",
  "version": "0.0.1",
  "description": "Capsula JS - web components",
  "scripts": {
    "bootstrap": "lerna bootstrap --npm-client=yarn",
    "build": "lerna run build",
    "start": "lerna run start",
    "lint": "lerna run lint",
    "test": "lerna run test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capsulajs/web-components.git"
  },
  "author": "Capsula JS",
  "bugs": {
    "url": "https://github.com/capsulajs/web-components/issues"
  },
  "homepage": "https://github.com/capsulajs/web-components#readme",
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/storybook__react": "^4.0.1",
    "babel-loader": "^8.0.5",
    "husky": "^1.3.1",
    "lerna": "^3.13.2",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "ts-loader": "^5.4.6",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@capsulajs/capsulahub-ui": "^0.5.0"
  }
}
