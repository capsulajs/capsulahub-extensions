{
  "name": "@capsulajs/capsulahub-extension-canvas",
  "version": "0.0.1",
  "description": "CapsulaJS - Web components canvas",
  "private": false,
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "es"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:lib && yarn build:dist",
    "build:dist": "rimraf dist && cross-env PACKAGE_NAME=canvas webpack",
    "build:lib": "rimraf lib && tsc",
    "clean": "rimraf lib dist dist-example node_modules",
    "start-example-server": "http-server --p 1234 --cors",
    "build-example": "rimraf public/dist && cross-env PACKAGE_NAME=canvas webpack --config webpack.config-example.js",
    "start-example": "yarn build-example && yarn start-example-server",
    "lint": "tslint -p tsconfig.json --fix",
    "prettier": "prettier --write '{src,tests}/**/*.{ts,tsx}'",
    "cypress:run": "cypress run --browser chrome",
    "cypress:debug": "cypress open",
    "test": "npm-run-all -p -r start-example-server cypress:run",
    "test:debug": "npm-run-all -p -r start-example-server cypress:debug",
    "deploy": "SERVICE=$(echo $npm_package_name | cut -d '/' -f 2) && ../../scripts/deploy.sh $SERVICE"
  },
  "author": "Markets Pulse",
  "dependencies": {
    "@capsulajs/capsulahub-extension-utils": "^0.0.1",
    "@capsulajs/capsulahub-ui": "latest",
    "rxjs": "6.5.1"
  }
}
