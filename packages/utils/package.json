{
  "name": "@capsulajs/web-components-utils",
  "version": "0.0.1",
  "description": "CapsulaJS - Web components utils",
  "private": false,
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "es"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules && rimraf .cache && rimraf lib && rimraf es",
    "build": "rimraf .cache && npm-run-all build:cjs build:es",
    "build:cjs": "rimraf lib && tsc --module commonjs --target es5 --outDir lib",
    "build:es": "rimraf es && tsc --module esnext --target es5 --outDir es",
    "lint": "tslint -p tsconfig.json --fix",
    "prettier": "prettier --write '{src,tests}/**/*.{ts,tsx}'"
  },
  "author": "Markets Pulse",
  "dependencies": {
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "rxjs": "^6.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@types/node": "^11.13.7",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "parcel-bundler": "^1.12.3",
    "tslint": "^5.15.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.3"
  }
}
