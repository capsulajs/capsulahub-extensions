{
  "name": "@capsulajs-web-components/request-form",
  "version": "0.0.1",
  "description": "CapsulaJS - Web components Request Form",
  "main": "lib/index.js",
  "unpkg": "dist/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules && rimraf .cache && rimraf lib && rimraf es",
    "build": "rimraf .cache && npm-run-all build:cjs build:es",
    "build:cjs": "rimraf lib && tsc --module commonjs --target es2015 --outDir lib",
    "build:es": "rimraf es && tsc --module esnext --target es2015 --outDir es",
    "cypress": "rimraf .cache && cypress run",
    "cypress:debug": "cypress open",
    "start": "rimraf dist && parcel start src/example/index.html",
    "lint": "tslint '{src,cypress}/**/*.{ts,tsx,js}' --fix",
    "prettier": "prettier --write '{src,cypress}/**/*.{ts,tsx,js}'",
    "test": "npm-run-all -p -r start cypress"
  },
  "author": "Markets Pulse",
  "dependencies": {
    "@capsulajs-web-components/utils": "0.0.1",
    "@capsulajs/capsulahub-ui": "latest",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@types/node": "^11.13.4",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "babel-core": "^6.26.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^2.6.3",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.3"
  }
}
