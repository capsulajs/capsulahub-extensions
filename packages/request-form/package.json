{
  "name": "@capsulajs/capsulahub-extension-request-form",
  "version": "0.0.1",
  "description": "CapsulaJS - Web components Request Form",
  "main": "lib/index.js",
  "unpkg": "dist/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "yarn build:lib && yarn build:dist",
    "build:dist": "rimraf dist && PACKAGE_NAME=$(echo \"${PWD##*/}\") webpack",
    "build:lib": "rimraf lib && tsc",
    "clean": "rimraf lib dist dist-example node_modules",
    "cypress": "rimraf .cache && cypress run",
    "start-example-server": "http-server './example' --p 1234 --cors",
    "build-example": "rimraf example/dist && PACKAGE_NAME=$(echo \"${PWD##*/}\") webpack --config webpack.config-example.js",
    "start-example": "yarn build-example && yarn start-example-server",
    "test": "npm-run-all -p -r start-example-server cypress",
    "lint": "tslint '{src,cypress}/**/*.{ts,tsx,js}' --fix",
    "prettier": "prettier --write '{src,cypress}/**/*.{ts,tsx,js}'",
    "cypress:debug": "cypress open"
  },
  "author": "Markets Pulse",
  "dependencies": {
    "@capsulajs/capsulahub-extension-utils": "^0.0.1",
    "@capsulajs/capsulahub-ui": "^0.6.6",
    "rxjs": "6.5.1"
  }
}
