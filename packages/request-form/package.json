{
  "name": "@capsulajs-web-components/request-form",
  "version": "0.0.1",
  "description": "CapsulaJS - Web components Request Form",
  "main": "lib/index.js",
  "unpkg": "dist/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "yarn build:lib && yarn build:dist",
    "build:dist": "rimraf dist && webpack",
    "build:lib": "rimraf lib && tsc",
    "clean": "rimraf lib dist dist-example node_modules",
    "cypress": "rimraf .cache && cypress run",
    "cypress:debug": "cypress open",
    "start": "rimraf dist && parcel start src/example/index.html",
    "lint": "tslint '{src,cypress}/**/*.{ts,tsx,js}' --fix",
    "prettier": "prettier --write '{src,cypress}/**/*.{ts,tsx,js}'",
    "test": "npm-run-all -p -r start cypress"
  },
  "author": "Markets Pulse",
  "dependencies": {
    "@capsulajs-web-components/utils": "0.0.1",
    "@capsulajs/capsulahub-ui": "latest",
    "rxjs": "^6.4.0"
  }
}
