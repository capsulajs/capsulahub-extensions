{
  "name": "@capsulajs/capsulahub-extension-table",
  "version": "0.0.1",
  "description": "CapsulaJS - Web components table",
  "private": false,
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "es"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:lib && yarn build:dist",
    "build:dist": "rimraf dist && cross-env PACKAGE_NAME=$(echo \"${PWD##*/}\") webpack",
    "build:lib": "rimraf lib && tsc",
    "clean": "rimraf lib dist dist-example node_modules",
    "cypress": "rimraf .cache && cypress run",
    "start-example-server": "http-server './example' --p 1234 --cors",
    "build-example": "rimraf example/dist && cross-env PACKAGE_NAME=$(echo \"${PWD##*/}\") webpack --config webpack.config-example.js",
    "start-example": "yarn build-example && yarn start-example-server",
    "lint": "tslint -p tsconfig.json --fix",
    "prettier": "prettier --write '{src,tests}/**/*.{ts,tsx}'",
    "test": "npm-run-all -p -r start-example-server cypress"
  },
  "author": "Markets Pulse",
  "dependencies": {
    "@capsulajs/capsulahub-extension-utils": "^0.0.1",
    "@capsulajs/capsulahub-ui": "^0.6.6",
    "rxjs": "6.5.1"
  }
}
