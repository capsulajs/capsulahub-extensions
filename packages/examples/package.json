{
  "name": "@capsulajs-web-components/examples",
  "version": "0.0.1",
  "description": "CapsulaJS - Web components Request Form",
  "main": "./dist/index.js",
  "types": "./lib/index.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build:es": "parcel build src/index.ts --out-dir dist",
    "build:types": "tsc src/index.ts --emit-declarations-only",
    "cypress": "components/request-form/cypress run",
    "start": "parcel index.html",
    "lint": "tslint -p tsconfig.json --fix",
    "prettier": "prettier --write '{src,cypress}/**/*.{ts,tsx}'",
    "test": "npm-run-all -p -r start components/request-form/cypress"
  },
  "author": "Markets Pulse",
  "dependencies": {
    "@capsulajs/capsulahub-ui": "^0.4.2",
    "@capsulajs-web-components/utils": "0.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@types/node": "^11.13.4",
    "cypress": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "tslint": "^5.15.0",
    "typescript": "^3.4.3"
  }
}
